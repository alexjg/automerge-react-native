{
  "name": "react-native-automerge",
  "description": "Automerge binding for React Native",
  "homepage": "https://automerge.org/",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/automerge/react-native-automerge",
    "directory": "package"
  },
  "license": "MIT",
  "author": "Automerge contributors",
  "contributors": [
    {
      "name": "Kr√¶n Hansen",
      "email": "mail@kraenhansen.dk"
    }
  ],
  "scripts": {
    "build": "tsc",
    "uniffi:checkout": "uniffi-bindgen-react-native checkout --config ubrn.config.yaml",
    "uniffi:android": "uniffi-bindgen-react-native build android --config ubrn.config.yaml --and-generate",
    "uniffi:ios": "uniffi-bindgen-react-native build ios --config ubrn.config.yaml --and-generate",
    "uniffi:clean": "rm -rf cpp/ android/src/main/java ios/ src/generated/ src/codegen/ *.xcframework *.podspec"
  },
  "peerDependencies": {
    "@automerge/automerge": "^2"
  },
  "codegenConfig": {
    "name": "RNAutomergeSpec",
    "type": "modules",
    "jsSrcsDir": "src/codegen",
    "outputDir": {
      "ios": "ios/generated",
      "android": "android/generated"
    },
    "android": {
      "javaPackageName": "org.automerge"
    }
  }
}